# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
#
# Parameters for Unravel Observability tool configuration in GCP
#
# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

# [Required] GCP Project IDs to configure Unravel Monitoring and the corresponding PubSub subscription ID.
# Please note to use the same name while running `manager config bigquery add <project> <unravel_subscription>`
# Default value is "unravel-bigquery-sub"
monitoring_project_ids = ["gcp-monitoring-project-1"]

# [Required] GCP Project ID where the Unravel is running. 
# Only required for VM and Single Auth model. Please remove for Multi auth model
svc_account_project_id = "unravel-project-id"

# [Optional] GCP admin project ID if using Collections and Reservations
#admin_project_ids = ["gcp-admin-project-1"]

billing_project_ids = ["billing-project-id"]

